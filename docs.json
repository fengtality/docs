{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Hummingbot API",
  "redirects": [
    {
      "source": "/",
      "destination": "/docs"
    }
  ],
  "colors": {
    "primary": "#00B1BB",
    "light": "#5FFFD7",
    "dark": "#15803D"
  },
  "favicon": "/images/hummingbot-black.png",
  "metadata": {
    "og:site_name": "Hummingbot API"
  },
  "api": {
    "playground": {
      "proxy": false
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "href": "/docs",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "docs",
              "docs/installation",
              "docs/quickstart"
            ]
          },
          {
            "group": "Gateway",
            "pages": [
              "docs/gateway-setup"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "href": "/api-reference",
        "openapi": "/api-reference/openapi.json",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference"
            ]
          },
          {
            "group": "Accounts",
            "pages": [
              "GET /accounts",
              "POST /accounts/add-account",
              "POST /accounts/delete-account",
              "GET /accounts/{account_name}/credentials",
              "POST /accounts/add-credential/{account_name}/{connector_name}",
              "POST /accounts/delete-credential/{account_name}/{connector_name}"
            ]
          },
          {
            "group": "Portfolio",
            "pages": [
              "POST /portfolio/state",
              "POST /portfolio/history",
              "POST /portfolio/distribution",
              "POST /portfolio/accounts-distribution"
            ]
          },
          {
            "group": "Trading",
            "pages": [
              "POST /trading/orders",
              "POST /trading/orders/active",
              "POST /trading/orders/search",
              "POST /trading/{account_name}/{connector_name}/orders/{client_order_id}/cancel",
              "POST /trading/positions",
              "POST /trading/trades",
              "POST /trading/funding-payments",
              "GET /trading/{account_name}/{connector_name}/position-mode",
              "POST /trading/{account_name}/{connector_name}/position-mode",
              "POST /trading/{account_name}/{connector_name}/leverage"
            ]
          },
          {
            "group": "Connectors",
            "pages": [
              "GET /connectors",
              "GET /connectors/{connector_name}/config-map",
              "GET /connectors/{connector_name}/trading-rules",
              "GET /connectors/{connector_name}/order-types"
            ]
          },
          {
            "group": "Market Data",
            "pages": [
              "POST /market-data/prices",
              "POST /market-data/candles",
              "POST /market-data/historical-candles",
              "POST /market-data/funding-info",
              "POST /market-data/order-book",
              "POST /market-data/order-book/price-for-volume",
              "POST /market-data/order-book/volume-for-price",
              "POST /market-data/order-book/vwap-for-volume",
              "POST /market-data/order-book/price-for-quote-volume",
              "POST /market-data/order-book/quote-volume-for-price",
              "GET /market-data/active-feeds",
              "GET /market-data/available-candle-connectors",
              "GET /market-data/settings"
            ]
          },
          {
            "group": "Rate Oracle",
            "pages": [
              "GET /rate-oracle/sources",
              "GET /rate-oracle/prices",
              "GET /rate-oracle/rate/{trading_pair}",
              "GET /rate-oracle/rate-async/{trading_pair}",
              "POST /rate-oracle/rates",
              "GET /rate-oracle/config",
              "PUT /rate-oracle/config"
            ]
          },
          {
            "group": "Bot Orchestration",
            "pages": [
              "GET /bot-orchestration/status",
              "GET /bot-orchestration/mqtt",
              "GET /bot-orchestration/bot-runs",
              "GET /bot-orchestration/bot-runs/stats",
              "GET /bot-orchestration/bot-runs/{bot_run_id}",
              "GET /bot-orchestration/{bot_name}/status",
              "GET /bot-orchestration/{bot_name}/history",
              "POST /bot-orchestration/start-bot",
              "POST /bot-orchestration/stop-bot",
              "POST /bot-orchestration/stop-and-archive-bot/{bot_name}",
              "POST /bot-orchestration/deploy-v2-controllers",
              "POST /bot-orchestration/deploy-v2-script"
            ]
          },
          {
            "group": "Controllers",
            "pages": [
              "GET /controllers",
              "GET /controllers/{controller_type}/{controller_name}",
              "POST /controllers/{controller_type}/{controller_name}",
              "DELETE /controllers/{controller_type}/{controller_name}",
              "GET /controllers/{controller_type}/{controller_name}/config/template",
              "POST /controllers/{controller_type}/{controller_name}/config/validate",
              "GET /controllers/configs",
              "GET /controllers/configs/{config_name}",
              "POST /controllers/configs/{config_name}",
              "DELETE /controllers/configs/{config_name}",
              "GET /controllers/bots/{bot_name}/configs",
              "POST /controllers/bots/{bot_name}/{controller_name}/config"
            ]
          },
          {
            "group": "Scripts",
            "pages": [
              "GET /scripts",
              "GET /scripts/{script_name}",
              "POST /scripts/{script_name}",
              "DELETE /scripts/{script_name}",
              "GET /scripts/{script_name}/config/template",
              "GET /scripts/configs",
              "GET /scripts/configs/{config_name}",
              "POST /scripts/configs/{config_name}",
              "DELETE /scripts/configs/{config_name}"
            ]
          },
          {
            "group": "Docker",
            "pages": [
              "GET /docker/running",
              "GET /docker/available-images",
              "GET /docker/active-containers",
              "GET /docker/exited-containers",
              "POST /docker/pull-image",
              "GET /docker/pull-status",
              "POST /docker/start-container/{container_name}",
              "POST /docker/stop-container/{container_name}",
              "POST /docker/remove-container/{container_name}",
              "POST /docker/clean-exited-containers"
            ]
          },
          {
            "group": "Backtesting",
            "pages": [
              "POST /backtesting/run-backtesting"
            ]
          },
          {
            "group": "Archived Bots",
            "pages": [
              "GET /archived-bots",
              "GET /archived-bots/{db_path}/status",
              "GET /archived-bots/{db_path}/summary",
              "GET /archived-bots/{db_path}/controllers",
              "GET /archived-bots/{db_path}/executors",
              "GET /archived-bots/{db_path}/orders",
              "GET /archived-bots/{db_path}/trades",
              "GET /archived-bots/{db_path}/positions",
              "GET /archived-bots/{db_path}/performance"
            ]
          }
        ]
      },
      {
        "tab": "Gateway Reference",
        "href": "/gateway-reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "gateway-reference"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Hummingbot Docs",
          "href": "https://hummingbot.org/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/hummingbot/hummingbot-api",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/hummingbot",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/hummingbot-api-light.svg",
    "dark": "/logo/hummingbot-api-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Discord",
        "href": "https://discord.gg/hummingbot"
      }
    ],
    "primary": {
      "type": "button",
      "label": "GitHub",
      "href": "https://github.com/hummingbot/hummingbot-api"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/hummingbot",
      "github": "https://github.com/hummingbot",
      "discord": "https://discord.gg/hummingbot"
    }
  }
}
